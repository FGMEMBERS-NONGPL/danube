<?xml version="1.0"?>
<!--== Danube class Runabout  version 38.0 ==-->

<PropertyList>
 <fx>
	<engine>
		<name>rumble</name>
		<mode>looped</mode>
		<path>Sounds/jet.wav</path>
		<property>velocities/airspeed-kt</property>
		<volume>
			<property>sim/model/danube/sound/engines-volume-level</property>
		</volume>
		<pitch>
			<property>sim/model/danube/sound/pitch-level</property>
		</pitch>
	</engine>

	<engine>
		<name>overdrive</name>
		<mode>looped</mode>
		<path>Sounds/jet.wav</path>
		<condition>
			<greater-than-equals>
				<property>velocities/airspeed-kt</property>
				<value>6000</value>
			</greater-than-equals>
		</condition>
		<volume>
			<property>engines/engine/speed-max-powerlevel</property>
			<factor>0.3</factor>
			<offset>-1.9</offset>
		</volume>
		<pitch>
			<property>engines/engine/speed-max-powerlevel</property>
			<factor>0.25</factor>
			<offset>-0.75</offset>
		</pitch>
	</engine>

