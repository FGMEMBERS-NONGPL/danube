<!-- ground contacts -->
	<landing>
		<name>vertical ground contact</name>
		<mode>once</mode>
		<path>Sounds/helicopter-skid.wav</path>
		<condition>
			<property>sim/model/danube/position/landing-wow</property>
		</condition>
	</landing>

	<contact-slow>
		<name>slow moving ground contact</name>
		<mode>looped</mode>
		<path>Sounds/squeal.wav</path>
		<condition>
			<property>sim/model/danube/position/squeal-wow</property>
		</condition>
		<volume>
			<property>velocities/abs-airspeed-kt</property>
			<factor>0.0005</factor>
			<max>0.1</max>
		</volume>
	</contact-slow>

	<contact-rub-1>
		<name>slowest moving ground contact once</name>
		<mode>once</mode>
		<path>Aircraft/danube/Sound/altitude-1.wav</path>
		<condition>
			<and>
				<less-than>
					<property>sim/model/danube/position/altitude-agl-ft</property>
					<value>1</value>
				</less-than>
				<greater-than>
					<property>velocities/abs-airspeed-kt</property>
					<value>1</value>
				</greater-than>
				<less-than>
					<property>velocities/abs-airspeed-kt</property>
					<value>10</value>
				</less-than>
			</and>
		</condition>
	</contact-rub-1>

	<contact-rubbing>
		<name>slowest moving ground contact</name>
		<mode>looped</mode>
		<path>Aircraft/danube/Sound/altitude-0-loop.wav</path>
		<condition>
			<and>
				<less-than>
					<property>sim/model/danube/position/altitude-agl-ft</property>
					<value>1</value>
				</less-than>
				<greater-than>
					<property>velocities/abs-airspeed-kt</property>
					<value>1</value>
				</greater-than>
			</and>
		</condition>
	</contact-rubbing>

	<contact-continuous>
		<name>continuous ground contact</name>
		<mode>looped</mode>
		<path>Sounds/rumble.wav</path>
		<condition>
			<and>
				<greater-than>
					<property>sim/model/danube/position/skid-wow</property>
					<value>0.05</value>
				</greater-than>
				<greater-than>
					<property>velocities/abs-airspeed-kt</property>
					<value>20</value>
				</greater-than>
			</and>
		</condition>
		<volume>
			<property>sim/model/danube/position/skid-wow</property>
			<min>0.0</min>
			<max>1.0</max>
		</volume>
		<pitch>
			<factor>0.01</factor>
		</pitch>
	</contact-continuous>

	<crash-shields-holding>
		<name>intense ground contact</name>
		<mode>once</mode>
		<path>Sounds/squeal.wav</path>
		<condition>
			<greater-than>
				<property>sim/model/danube/position/skid-wow</property>
